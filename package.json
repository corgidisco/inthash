{
  "name": "inthash",
  "version": "1.1.0",
  "description": "Integer Hashing Library based on Knuth's multiplicative hashing method for Javascript(& Typescript).",
  "keywords": [
    "obfuscate",
    "obfuscation",
    "knuth",
    "uuid",
    "hash",
    "optimus",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/inthash.git"
  },
  "bin": {
    "inthashgen": "lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*.ts",
    "test:unit": "jest"
  },
  "dependencies": {
    "big-integer": "^1.6.38"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "25.2.3",
    "@types/node": "10.17.27",
    "eslint": "7.4.0",
    "eslint-config-stable": "0.8.0",
    "jest": "26.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.2",
    "typescript": "3.9.7"
  }
}
