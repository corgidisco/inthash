{
  "name": "inthash",
  "version": "1.1.0",
  "description": "Integer Hashing Library based on Knuth's multiplicative hashing method for Javascript(& Typescript).",
  "keywords": [
    "obfuscate",
    "obfuscation",
    "knuth",
    "uuid",
    "hash",
    "optimus",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/inthash.git"
  },
  "bin": {
    "inthashgen": "lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "lint": "tslint -c ./tslint.json -p tsconfig.json 'src/**/*.ts' --format stylish",
    "jest": "jest test",
    "test": "npm run build && npm run lint && npm run jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "big-integer": "^1.6.38"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@types/jest": "24.9.1",
    "@types/node": "10.17.14",
    "jest": "25.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "24.3.0",
    "tslint": "5.20.1",
    "tslint-language-service": "0.9.9",
    "typescript": "3.7.5"
  }
}
